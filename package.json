{
	"name": "nanometer-message",
	"sideEffects": false,
	"version": "1.5.0",
	"description": "Simple message prompts",
	"main": "dist/index.js",
	"style": "dist/mian.css",
	"scripts": {
		"serve": "webpack-dev-server --open --mode=development",
		"build": "webpack --mode=development",
		"test": "echo \"Error: no test specified\" && exit 1",
		"cleanLib": "./node_modules/.bin/rimraf lib",
		"bableBuild": "./node_modules/.bin/babel src --out-dir lib",
		"bableSass": "sass src/style.scss:lib/style.css --style compressed",
		"prepublish": "npm run cleanLib && npm run bableBuild && npm run bableSass ",
		"webpackBuild": "webpack --config webpack.config.js",
		"mochaUI": "mocha-phantomjs --path ./text/index.js ./test/index.html"
	},
	"files": [
		"lib",
		"src"
	],
	"keywords": [
		"message",
		"nanometer-message",
		"nanometerMessage"
	],
	"author": "nanomster",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"autoprefixer": "^10.4.7",
		"babel-cli": "^6.26.0",
		"babel-loader": "^8.2.5",
		"babel-preset-es2015": "^6.24.1",
		"baby-ajax": "^1.6.13",
		"braces": "^3.0.2",
		"chai": "^4.3.6",
		"css-loader": "^6.7.1",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.6.0",
		"mixin-deep": "^2.0.1",
		"mocha-phantomjs": "^4.1.0",
		"mocha-phantomjs-core": "^2.1.2",
		"postcss": "^8.4.14",
		"postcss-loader": "^7.0.0",
		"prettier": "^2.6.2",
		"rimraf": "^2.7.1",
		"sass": "^1.52.1",
		"sass-loader": "^13.0.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.9.0"
	},
	"dependencies": {
		"@webpack-cli/generators": "^2.4.2",
		"clean-webpack-plugin": "^4.0.0",
		"dart-sass": "^1.25.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"mocha": "^6.2.3",
		"resource-hints-webpack-plugin": "^0.0.2",
		"source-map-loader": "^3.0.1",
		"webpack-bundle-analyzer": "^4.5.0"
	}
}
